#!/usr/bin/sh

SOCKET_PATH=ipc:///tmp/synthpod_bin
if [ "$#" -eq 1 ]; then
	SOCKET_PATH=$1
fi

synthpod_sandbox_efl \
	-p http://open-music-kontrollers.ch/lv2/synthpod#stereo \
	-b /usr/local/lib/lv2/synthpod.lv2/ \
	-u http://open-music-kontrollers.ch/lv2/synthpod#common_3_eo \
	-s $SOCKET_PATH \
	-w "Synthpod" \
	-r 48000
