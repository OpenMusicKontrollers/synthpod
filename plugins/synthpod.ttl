# Atom2Control Plugin
synthpod:atom2control
	a lv2:Plugin ,
		lv2:ConverterPlugin ;
	doap:name "Synthpod Atom2Control" ;
	doap:license lic:Artistic-2.0 ;
	lv2:project proj:synthpod ;
	lv2:requiredFeature bufsz:boundedBlockLength, urid:map ;
	lv2:optionalFeature lv2:isLive , lv2:hardRTCapable ;

	lv2:port [
		# atom_in
	  a lv2:InputPort ,
			atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports atom:Tuple ;
		lv2:index 0 ;
		lv2:symbol "atom_in" ;
		lv2:name "Atom In" ;
		pg:group synthpod:group_in ;
		lv2:designation lv2:control ;
	] , [
		# input
	  a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 1 ;
		lv2:symbol "offset" ;
		lv2:name "Offset" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 240.0 ;
		lv2:portProperty lv2:integer, lv2:enumeration ;
